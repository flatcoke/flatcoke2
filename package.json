{
  "name": "custom-server-express",
  "version": "1.0.0",
  "scripts": {
    "dev": "NODE_ENV=development nodemon ./server --exec babel-node --presets @babel/preset-env --watch server",
    "dev:debug": "NODE_ENV=development nodemon --inspect=5858 ./server --exec babel-node --presets @babel/preset-env --watch server",
    "build": "yarn build:next && yarn build:server",
    "build:next": "next build client",
    "build:server": "NODE_ENV=production babel server --out-dir .next/server/dist",
    "start": "NODE_ENV=production node .next/server/dist/index.js"
  },
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "@zeit/next-sass": "^1.0.1",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "dotenv-webpack": "^1.6.0",
    "express": "^4.14.0",
    "include-media": "^1.4.9",
    "jss": "^9.8.7",
    "next": "^7.0.2",
    "next-redux-wrapper": "^3.0.0-alpha.0",
    "node-sass": "^4.11.0",
    "open-color": "^1.6.3",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth": "^1.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-jss": "^8.6.1",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-module-resolver": "^3.1.3",
    "eslint": "^5.13.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.3",
    "nodemon": "^1.18.9",
    "prettier": "^1.16.3",
    "pretty-quick": "^1.10.0"
  }
}
